#!/usr/bin/bash

  check_number_of_args() {
	  echo $must_n $n_inParams $abs_way
  if [ $must_n != $n_inParams ]
	then echo -e "Error!\nMust be $must_n parameters"
 	exit 1
  fi
}

  check_absolute_way() {
  echo absolut way=$abs_way
}

  check_args() {
  check_number_of_args $n_inParams
  check_absolute_way
}

