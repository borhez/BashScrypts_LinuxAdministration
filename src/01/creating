generate_name()
{
  if [ $# -eq 2 ]
  	then str=file.ads
  	echo $str
  fi

  if [ $# -eq 1 ]
	then str=folder.ser
	echo $str
  fi
}

create_newfile()
{
  new_fname=$(generate_name $letters_f_name $letters_f_ext)
  echo "in crNewFile() newname=$new_fname"
}

create_newfolder()
{
	new_dname=$(generate_name $d_letters)
	echo "in crNewFolder(() newname=$new_dname"
}
